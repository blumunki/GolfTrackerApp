<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:GolfTrackerApp.Mobile"
             xmlns:components="clr-namespace:GolfTrackerApp.Mobile.Components"
             x:Class="GolfTrackerApp.Mobile.MainPage"
             Title="Golf Tracker">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type components:App}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
